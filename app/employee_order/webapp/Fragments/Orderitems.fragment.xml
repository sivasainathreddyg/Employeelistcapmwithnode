<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:form="sap.ui.layout.form">
    <Dialog
        id="OrderDetailDialog"
        title="Order Details">
        <content>
            <form:SimpleForm id="OrderDetail">
                <form:content>
                    <Label text="orderId" />
                    <Input value="{orderId}" id="orderId" />

                    <Label text="Employee ID"  />
                    <Input value="{employeeId}" id="empid" />

                    <Label text="orderName"  />
                    <Input value="{orderName}" id="orderName"/>

                     <Label text="orderPrice"  />
                    <Input value="{orderPrice}" id="orderPrice"/>

                    <Label text="Created Date"></Label>
                    <Input value="{createdDate}" id="createdDate"></Input>
                </form:content>
            </form:SimpleForm>
        </content>
        <buttons>
            
                <Button text="Close" type="Reject" press="onCloseO"/>
                <Button text="Submit" type="Accept" press="onsubmitorderdetails" />
           
        </buttons>
    </Dialog>
</core:FragmentDefinition>
